AEA,IAAA,QAAA,GAAA,IAAA,KAAA,CAAA,iBAAA,CAAA,CAAA,EAAA,CAAA,EAAA,CAAA,CAAA;AACA,IAAA,QAAA,GAAA,IAAA,KAAA,CAAA,iBAAA,EAAA;AACA,IAAA,IAAA,GAAA,IAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EAAA,QAAA,CAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,OADA;AAEA,EAAA,UAAA,EAAA,EAFA;AAGA,EAAA,OAHA,qBAGA;AACA,IAAA,KAAA,CAAA,UAAA,GAAA,IAAA,KAAA,CAAA,KAAA,CAAA,SAAA,CAAA;AACA,IAAA,KAAA,CAAA,GAAA,CAAA,IAAA;AACA,IAAA,QAAA,CAAA,OAAA,CAAA,WAAA,EAAA,YAAA;AACA,IAAA,QAAA,CAAA,aAAA,CAAA,MAAA,CAAA,gBAAA;AACA,IAAA,MAAA,CAAA,QAAA,CAAA,GAAA,CAAA,CAAA,EAAA,CAAA,EAAA,EAAA;AACA,IAAA,SAAA,CAAA,WAAA,CAAA,QAAA,CAAA,UAAA;;AAEA,QAAA,KAAA,CAAA,gBAAA,EAAA,EAAA,CACA;AACA;AACA,KAHA,MAGA;AACA,UAAA,OAAA,GAAA,KAAA,CAAA,oBAAA,EAAA;AACA,MAAA,SAAA,CAAA,WAAA,CAAA,OAAA;AACA;AACA;AAlBA,CAAA","sourcesContent":["<template>\n    <div id=\"scene-container\">\n    </div>\n</template>\n\n<script>\n    import * as THREE from 'three';\n    import WEBGL from '../mixins/webgl';\n\n    const container = document.querySelector('#scene-container');\n    const scene = new THREE.Scene();\n    const { clientHeight, clientWidth } = container;\n    // camera\n    const fov = 35; // AKA Field of View\n    const aspect = clientWidth / clientHeight;\n    const near = 0.1; // the near clipping plane\n    const far = 100; // the far clipping plane\n    const camera = new THREE.PerspectiveCamera(fov, aspect, near, far);\n    const renderer = new THREE.WebGLRenderer();\n\n    const geometry = new THREE.BoxBufferGeometry(2, 2, 2);\n    const material = new THREE.MeshBasicMaterial();\n    const mesh = new THREE.Mesh(geometry, material);\n\n    export default {\n        name: 'Scene',\n        components: {},\n        mounted() {\n            scene.background = new THREE.Color('skyblue');\n            scene.add(mesh);\n            renderer.setSize(clientWidth, clientHeight);\n            renderer.setPixelRatio(window.devicePixelRatio);\n            camera.position.set(0, 0, 10);\n            container.appendChild(renderer.domElement);\n\n            if (WEBGL.isWebGLAvailable()) {\n                // Initiate function or other initializations here\n                // animate();\n            } else {\n                const warning = WEBGL.getWebGLErrorMessage();\n                container.appendChild(warning);\n            }\n        },\n    };\n</script>\n\n<style lang=\"scss\">\n    #scene-container {\n        font-family: Avenir, Helvetica, Arial, sans-serif;\n        -webkit-font-smoothing: antialiased;\n        -moz-osx-font-smoothing: grayscale;\n        text-align: center;\n        color: #2c3e50;\n        margin-top: 0;\n        position: absolute;\n        width: 100%;\n        height: 100%;\n    }\n\n    #info {\n        position: absolute;\n        top: 10px;\n        width: 100%;\n        text-align: center;\n        z-index: 100;\n        display:block;\n    }\n</style>\n"],"sourceRoot":"src/components"}]}